name: Process DEV Artifact
on:
  push:
    branches:
      - STAGE

jobs:
  process-artifact:
    runs-on: ubuntu-latest

    steps:
      # Download the release asset containing the output file
      - name: Download Artifact
        uses: actions/download-artifact@v2
        with:
          repository: plainIDusecase/TestAPIPipeline
          name: DEV-output
          path: output

      - name: Print Output
        run: cat output/DEVout.txt
